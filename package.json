{
  "name": "qui-z",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 8"
  },
  "main": "src/server/start.js",
  "now": {
    "dotenv": "variables.env"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./src/server/start.js",
    "build": "webpack --mode production",
    "client": "NODE_ENV=development webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "NODE_ENV=development nodemon ./src/server/start.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test-client": "NODE_ENV=test jest --watch",
    "test-server": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests/server/**.js",
    "test": "npm run test-server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "celebrate": "^8.0.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "es6-promisify": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "mongoose": "^5.1.4",
    "mongoose-mongodb-errors": "^0.0.2",
    "passport": "^0.4.0",
    "passport-local-mongoose": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.7",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-react-session": "^2.4.0",
    "redux-thunk": "^2.3.0",
    "slugs": "^0.1.3",
    "supertest": "^3.1.0",
    "unexpected": "^10.38.0",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.20.3",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "rootDir": ".",
    "verbose": true,
    "testRegex": "./tests/client/.*.test.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/client/__mocks__/styleMock.js"
    }
  }
}
